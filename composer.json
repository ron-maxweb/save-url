{
    "name": "maxweb/save-url",
    "description": "Laravel 5 package to redirect users to the last visited page on login.",
    "keywords": [
        "maxweb",
        "laravel",
        "login",
        "save-url",
        "redirect after login"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Ron Marsden",
            "email": "ron.marsden@maxwebsolutions.co.uk",
            "homepage": "https://maxwebsolutions.co.uk"
        }
    ],
    "require": {
        "illuminate/support": "^5.5",
        "illuminate/config": "^5.5",
        "illuminate/session": "^5.5"
    },
    "require-dev": {
        "phpunit/phpunit" : "^4",
        "orchestra/testbench": "^3.1",
        "mockery/mockery": "^0.9.4"
    },
    "autoload": {
        "psr-4": {
            "Maxweb\\SaveUrl\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Maxweb\\SaveUrl\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Maxweb\\SaveUrl\\SaveUrlServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}